{
    "manifest_version": 2,
    "name": "Video Grabber",
    "version": "1.0",
    "permissions": ["activeTab", "storage", "downloads"],
    "browser_action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      },
      "default_title": "Download Videos"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  